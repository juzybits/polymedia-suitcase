{
    "private": false,
    "version": "0.0.4",
    "name": "@polymedia/suits",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://github.com/juzybits/polymedia-suits",
    "description": "Sui TypeScript utilities",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-suits"
    },
    "keywords": [
        "polymedia",
        "sui",
        "utilities"
    ],
    "scripts": {
        "build": "rm -rf dist/ && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "@mysten/sui.js": "^0.49.1"
    },
    "devDependencies": {
        "@types/node": "^20.11.4",
        "typescript": "^5.3.3"
    },
    "type": "module",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/",
        "src/",
        "common/",
        "node/",
        "LICENSE",
        "README.md"
    ],
    "exports": {
        "./common": {
            "source": "./src/common/index.ts",
            "import": "./dist/esm/common/index.js",
            "require": "./dist/cjs/common/index.js"
        },
        "./node": {
            "source": "./src/node/index.ts",
            "import": "./dist/esm/node/index.js",
            "require": "./dist/cjs/node/index.js"
        }
    }
}
