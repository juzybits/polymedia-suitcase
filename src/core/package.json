{
	"private": false,
	"version": "0.0.65",
	"name": "@polymedia/suitcase-core",
	"author": "@juzybits (https://polymedia.app)",
	"homepage": "https://github.com/juzybits/polymedia-suitcase",
	"description": "Sui TypeScript utilities",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/juzybits/polymedia-suitcase"
	},
	"keywords": [
		"polymedia",
		"sui",
		"suitcase",
		"core"
	],
	"scripts": {
		"build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
		"clean": "rm -rf dist/ node_modules/ .turbo/ .wrangler/",
		"dev": "tsc --watch -p tsconfig.esm.json",
		"lint:fix": "biome check --write *.* src/",
		"lint:unsafe": "biome check --write --unsafe *.* src/",
		"lint": "biome check *.* src/",
		"test": "jest --verbose --passWithNoTests",
		"typecheck": "tsc -p tsconfig.esm.json"
	},
	"peerDependencies": {
		"@mysten/sui": "^1.37.4"
	},
	"devDependencies": {},
	"type": "module",
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist/"
	],
	"types": "./dist/esm/index.d.ts",
	"module": "./dist/esm/index.js",
	"main": "./dist/cjs/index.js",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	}
}
